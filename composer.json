{
    "name": "hetao/hetao-module",
    "description": "hetao modules",
    "license": "MIT",
    "authors": [
        {
            "name": "hetao",
            "email": "hetao4ai@outlook.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
